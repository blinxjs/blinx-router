{
  "name": "blinx-router",
  "version": "0.9.7",
  "description": "Router for Blinx Applications.",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack src/index.js dist/router.js",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack src/index.js dist/router.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blinxjs/blinx-router.git"
  },
  "keywords": [
    "Blinx",
    "BlinxJs",
    "Router",
    "Router5"
  ],
  "author": "truss-developers@flipkart.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blinxjs/blinx-router/issues"
  },
  "homepage": "https://github.com/blinxjs/blinx-router#readme",
  "dependencies": {
    "router5": "5.1.2",
    "router5-link-interceptor": "3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "cross-env": "1.0.8",
    "webpack": "1.13.1"
  }
}
